height: 0;\n     color: transparent;\n     background: transparent;\n     font-family: 'Microsoft JhengHe', sans-serif, Arial;\n     text-decoration: none;\n     text-shadow: none;\n     letter-spacing: 0.9px;\n}\n #header h1 a::before {\n     content: \"超形而上之谬¿\";\n     font-size: 90%;\n     color: #e6e6e6;\n     color: var(--bg);\n     letter-spacing: -0.02em;\n     text-shadow: 3px 3px 5px black;\n}\n #header h1 a span{\n     content: \"\";\n     visibility: hidden;\n}\n #header h2 span {\n     display: block;\n     margin: 0;\n     padding: 19px 0;\n     line-height: 0;\n     max-height: 0;\n     font-weight: bold;\n     color: transparent;\n     text-shadow: none;\n}\n #header h2 span::before {\n     content: \"……我还未想杀死你\";\n     color: #e6e6e6;\n     color: var(--bg);\n     text-shadow: 1px 1px 1px black;\n     text-shadow: 1px 1px 1px rgba(0,0,0,.8);\n}\n #header span {\n     color: #e6e6e6;\n     color: var(--bg);\n}\n \n/* ADJUST MOBILE IMAGE SIZE */\n @media (max-width: 767px) {\n     .open-menu a {\n         border: 0.2em solid #3e2659 !important;\n         border: 0.2em solid var(--fg-muted-dark) !important;\n         background-color: white !important;\n         color: #3e2659 !important;\n         color: var(--fg-muted-dark) !important;\n    }\n     #side-bar {\n         background-color: rgb(192, 192, 192);\n    }\n     #side-bar:target + #main-content {\n         left: 0;\n    }\n}\n @media (min-width: 768px) {\n     #header h1 a {\n         font-size: 150%;\n    }\n}\n \n/* TOP MENU */\n #top-bar ul li ul {\n     border: solid 1px #3e2659;\n     border: solid 1px var(--fg-muted-dark);\n}\n #top-bar ul .sfhover a,\n #top-bar ul li:hover a {\n     background: #e6e6e6;\n     background: var(--bg);\n     color: #3e2659;\n     color: var(--fg-muted-dark);\n}\n #top-bar a {\n     color: #eee;\n     color: var(--page-title);\n}\n #login-status ul a {\n     color: #5b2f8e;\n     color: var(--fg-light);\n}\n #my-account,\n #account-topbutton {\n     color: #